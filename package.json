{
  "name": "honeycomb",
  "description": "all-in-one task starter app with jsPsych + React + Electron + psiturk ",
  "author": {
    "name": "Brown CCV",
    "email": "ccv-bot@brown.edu",
    "url": "ccv.brown.edu"
  },
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "repository": "https://github.com/brown-ccv/honeycomb",
  "dependencies": {
    "@brown-ccv/behavioral-task-trials": "^1.0.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "bootstrap": "^4.3.1",
    "electron-log": "^3.0.9",
    "electron-squirrel-startup": "^1.0.0",
    "event-marker": "git+https://github.com/brown-ccv/event-marker.git",
    "execa": "^5.0.0",
    "firebase": "^8.2.6",
    "jquery": "^3.4.1",
    "jspsych-react": "^0.2.2",
    "lodash": "^4.17.19",
    "popper.js": "^1.16.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "^3.6.4"
  },
  "scripts": {
    "commit": "git-cz",
    "postinstall": "node version.js && npm run rebuild",
    "start": "cross-env BROWSER=\"none\" NODE_PATH=\"./src\" react-scripts start",
    "start:browser": "react-scripts start",
    "test": "react-scripts test",
    "prebuild": "electron-rebuild",
    "build": "react-scripts build",
    "package:windows": "electron-packager . --platform win32 --arch x64 --icon ./assets/icons/win/icon.ico --out dist/ --overwrite --asar ",
    "package:mac": "electron-packager . --platform darwin --arch x64 --out dist/ --icon ./assets/icons/mac/icon.icns --overwrite",
    "package:linux": "electron-packager . --platform linux --arch x64 --icon ./assets/icons/mac/icon.icns --out dist/ --overwrite",
    "postpackage:windows": "electron-installer-windows --src dist/%npm_package_name%-win32-x64/ --dest dist/installers/ --overwrite --homepage https://ccv.brown.edu/",
    "postpackage:linux": "electron-installer-debian --src dist/${npm_package_name}-linux-x64/ --dest dist/installers/ --arch x64 --overwrite",
    "installer:mac": "electron-installer-dmg ./dist/${npm_package_name}-darwin-x64/${npm_package_name}.app ${npm_package_name}-${npm_package_version} --out ./dist/installers/ --icon ./assets/icons/mac/icon.icns --overwrite",
    "rebuild": "electron-rebuild",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-config:home": "echo '{\"eventMarker\": false}' > electron_config.json",
    "electron-config:clinic": "echo '{\"eventMarker\": true}' > electron_config.json",
    "electron-dev": "cross-env ELECTRON_START_URL=\"http://localhost:3000\" electron .",
    "firebase:dev": "firebase emulators:start --import emulator_data",
    "firebase:savedev": "firebase emulators:export emulator_data",
    "dev": "concurrently --kill-others \"npm:start\" \"sleep 10 && npm run electron-dev\"",
    "dev:home": "npm run electron-config:home && dotenv -e env/.env.home npm run dev",
    "dev:home:video": "npm run electron-config:home && dotenv -e env/.env.home dotenv -e env/.env.video npm run dev",
    "dev:clinic:video": "npm run electron-config:clinic && dotenv -e env/.env.clinic dotenv -e env/.env.video npm run dev",
    "dev:clinic": "npm run electron-config:clinic && dotenv -e env/.env.clinic npm run dev",
    "dev:firebase": "dotenv -e env/.env.firebase npm run dev",
    "dev:turk-prolific": "dotenv -e env/.env.turk-prolific npm run dev",
    "build:turk-prolific": "dotenv -e env/.env.turk-prolific npm run build",
    "build:home:video": "npm run electron-config:home && dotenv -e env/.env.home dotenv -e env/.env.video npm run build",
    "build:clinic:video": "npm run electron-config:clinic && dotenv -e env/.env.clinic dotenv -e env/.env.video npm run build",
    "build:home": "npm run electron-config:home && dotenv -e env/.env.home npm run build",
    "build:clinic": "npm run electron-config:clinic && dotenv -e env/.env.clinic npm run build",
    "build:firebase": "dotenv -e env/.env.firebase npm run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-macros": "^2.6.1",
    "chai": "^4.2.0",
    "concurrently": "^4.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "electron": "6.1.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^14.0.6",
    "electron-rebuild": "^1.8.6",
    "firebase-cli": "^1.2.0",
    "firebase-tools": "^9.12.1",
    "require-context.macro": "^1.1.1"
  },
  "babel": {
    "plugin": [
      "macros"
    ]
  },
  "optionalDependencies": {
    "electron-installer-debian": "^2.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-installer-windows": "^2.0.0"
  }
}
