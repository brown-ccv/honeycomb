{
  "name": "honeycomb",
  "description": "all-in-one task starter app with jsPsych + React + Electron + psiturk ",
  "author": {
    "name": "Brown CCV",
    "email": "ccv-bot@brown.edu",
    "url": "https://ccv.brown.edu"
  },
  "version": "3.1.1",
  "license": "MIT",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "repository": "https://github.com/brown-ccv/honeycomb",
  "dependencies": {
    "@brown-ccv/behavioral-task-trials": "^2.0.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@jspsych/plugin-fullscreen": "^1.2.0",
    "@jspsych/plugin-html-button-response": "^1.1.1",
    "@jspsych/plugin-html-keyboard-response": "^1.1.1",
    "@jspsych/plugin-html-slider-response": "^1.1.2",
    "@jspsych/plugin-instructions": "^1.1.3",
    "@jspsych/plugin-survey-multi-select": "^1.1.1",
    "bootstrap": "^5.2.0-beta1",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "event-marker": "github:brown-ccv/event-marker",
    "execa": "^5.0.0",
    "firebase": "^10.1.0",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "jspsych": "^7.2.3",
    "lodash": "^4.17.19",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "require-context.macro": "^1.2.2",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-react": "^7.22.5",
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-macros": "^2.4.2",
    "babel-plugin-transform-require-context": "^0.1.1",
    "chai": "^4.2.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv-cli": "^7.0.0",
    "electron": "^26.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "firebase-admin": "^11.0.1",
    "firebase-tools": "^12.4.7",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "commit": "git-cz",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "build:clinic": "dotenv -e env/.env.production.clinic npm run build",
    "build:online": "dotenv -e env/.env.production.online npm run build",
    "dev:clinic": "dotenv -e env/.env.dev.clinic npm run start:electron",
    "dev:online": "dotenv -e env/.env.dev.online npm run start:browser",
    "electron": "wait-on http://127.0.0.1:3000 && cross-env ELECTRON_START_URL=\"http://127.0.0.1:3000\" electron-forge start",
    "firebase:emulators:start": "firebase emulators:start --import emulator_data",
    "firebase:emulators:save": "firebase emulators:export emulator_data",
    "firebase:download": "dotenv -e env/.env.firebase-download node download-response-data.js",
    "format": "npm run lint -- --fix",
    "postinstall": "node version.js && electron-rebuild",
    "lint": "eslint .",
    "make": "electron-forge make",
    "make:windows": "react-scripts build && npm run make -- --arch x64 --targets @electron-forge/maker-squirrel",
    "make:linux": "react-scripts build && npm run make -- --arch x64 --targets @electron-forge/maker-deb",
    "make:mac": "react-scripts build && npm run make -- --arch x64 --targets @electron-forge/maker-dmg",
    "make:zip": "react-scripts build && npm run make -- --arch x64 --targets @electron-forge/maker-zip",
    "prepare": "husky install",
    "package": "electron-forge package",
    "start": "react-scripts start",
    "start:browser": "npm run start",
    "start:electron": "concurrently -k npm:start npm:electron",
    "test": "react-scripts test"
  },
  "babel": {
    "plugins": [
      "macros"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
