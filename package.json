{
  "name": "honeycomb",
  "description": "All-in-one task starter app with jsPsych + React + Electron",
  "version": "4.0.0",
  "honeycombVersion": "",
  "author": {
    "name": "Brown CCV",
    "email": "ccv-bot@brown.edu",
    "url": "https://ccv.brown.edu"
  },
  "keywords": [
    "react",
    "vite",
    "electron",
    "electron-forge",
    "jsPsych"
  ],
  "repository": "https://github.com/brown-ccv/honeycomb",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": ".vite/build/main.cjs",
  "homepage": ".",
  "dependencies": {
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@jspsych/plugin-audio-keyboard-response": "^1.1.3",
    "@jspsych/plugin-fullscreen": "^1.2.0",
    "@jspsych/plugin-html-button-response": "^1.1.1",
    "@jspsych/plugin-html-keyboard-response": "^1.1.1",
    "@jspsych/plugin-image-keyboard-response": "^1.1.2",
    "@jspsych/plugin-initialize-camera": "^1.0.1",
    "@jspsych/plugin-instructions": "^1.1.3",
    "@jspsych/plugin-preload": "^1.1.2",
    "@jspsych/plugin-survey": "^1.0.0",
    "bootstrap": "^5.2.0-beta1",
    "electron-log": "^5.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "execa": "^8.0.1",
    "firebase": "^10.11.0",
    "fs-extra": "^11.1.1",
    "jspsych": "^7.2.3",
    "lodash": "^4.17.19",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "serialport": "^12.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@inquirer/prompts": "^3.0.3",
    "@vitejs/plugin-react": "^4.0.4",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv-cli": "^7.0.0",
    "electron": "^30.0.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "firebase-admin": "^12.0.0",
    "firebase-tools": "^13.1.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.0.2",
    "prettier": "^3.2.5",
    "vite": "^5.2.11"
  },
  "scripts": {
    "start:browser": "vite --port 3000",
    "start:electron": "electron-forge start",
    "dev:home": "dotenv -e env/.env.home npm run start:electron",
    "dev:home:video": "dotenv -e env/.env.home dotenv -e env/.env.video npm run start:electron",
    "dev:clinic": "dotenv -e env/.env.clinic npm run start:electron",
    "dev:clinic:video": "dotenv -e env/.env.clinic dotenv -e env/.env.video npm run start:electron",
    "dev:turk-prolific": "dotenv -e env/.env.turk-prolific npm run start:electron",
    "dev:firebase": "dotenv -e env/.env.firebase npm run start:browser",
    "firebase:emulators:start": "firebase emulators:start --import emulator_data",
    "firebase:emulators:save": "firebase emulators:export emulator_data",
    "build": "vite build",
    "build:home": "dotenv -e env/.env.home npm run build",
    "build:home:video": "dotenv -e env/.env.home dotenv -e env/.env.video npm run build",
    "build:clinic": "dotenv -e env/.env.clinic npm run build",
    "build:clinic:video": "dotenv -e env/.env.clinic dotenv -e env/.env.video npm run build",
    "build:firebase": "dotenv -e env/.env.firebase npm run build",
    "build:turk-prolific": "dotenv -e env/.env.turk-prolific npm run build",
    "make": "electron-forge make",
    "make:windows": "electron-forge make --arch x64 --targets @electron-forge/maker-squirrel",
    "make:mac-x64": "electron-forge make  --arch x64 --targets @electron-forge/maker-dmg",
    "make:mac-arm64": "electron-forge make  --arch arm64 --targets @electron-forge/maker-dmg",
    "make:linux": "electron-forge make --arch x64 --targets @electron-forge/maker-deb",
    "cli": "node cli.mjs",
    "commit": "git-cz",
    "format": "prettier --write .",
    "lint": "eslint .",
    "postinstall": "node version.js",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
