{
  "name": "honeycomb",
  "description": "All-in-one task starter app with jsPsych + React + Electron + psiturk ",
  "author": {
    "name": "Brown CCV",
    "email": "ccv-bot@brown.edu",
    "url": "https://ccv.brown.edu"
  },
  "honeycombVersion": "",
  "version": "3.3.0",
  "repository": "https://github.com/brown-ccv/honeycomb",
  "license": "MIT",
  "private": true,
  "main": "public/electron/main.js",
  "homepage": ".",
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.0.2",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "format": "npx prettier --write .",
    "prepare": "husky",
    "index": "node src/index.js",
    "dataStructures": "node src/dataStructures.js",
    "conditionals": "node src/conditionals.js",
    "loops": "node src/loops.js",
    "functions": "node src/functions.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
